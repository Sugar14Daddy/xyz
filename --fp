	Name = "Smack Talk",
	Default = "",
	Options = {"You Slow!", "Ezzzz Clap!","Your a Noob!", "Get Good Faq","Top 1 Spammer Ezzzz!"},
	Callback = function(Value)
    if Value == 'You Slow!' then
        for i = 1,7 do
            game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("You Slow!","All")
            end
    elseif Value == 'Ezzzz Clap!' then
        for i = 1,7 do
            game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("Ezzzz Clap!","All")
            end
    elseif Value == 'Your a Noob!' then
        for i = 1,7 do
            game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("Your a Noob!","All")
        end
     elseif Value == 'Get Good Faq' then
        for i = 1,7 do
            game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("Get Good Faq!","All")
        end
    elseif Value == 'Top 1 Spammer Ezzzz!' then
        for i = 1,7 do
            game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("Top 1 Spammer Ezzzz!","All")

        end 
    end
end
