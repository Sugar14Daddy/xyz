MorphsTab:AddDropdown({
	Name = "Bundle",
	Default = "",
	Options = {"Korblox Mage","Fancy Mr. Fox","Reinforced Crate Body","Frost Guard General","Mushirio Nightwatchman","Chef Tortrdee","Axie","Zara Larsson - Lay All Your Love On Me","Bazooka Bones","Malivinius","KSI","Theo Oculos, CEO","Triple-Headed Trouble","Sporeling","Red Panda","Creature From the Blox Lagoon","Longlong the Lucky Ox","Nick Bass","Circuit Breaker","Sun Slayer","Vacation Rex","Blue Collar Cat","Cyclock","Crazy Clown","Mr. Skeleton","Neon Party Guy","Temple Idol","Gothic Gargoyle","Cyborg Shogun","Lucky Gatito","Rusty & Copper","Gryphon","Merfin the Great","Elemental Stone Golem","Clawed Companion","Old Town Road LNX - Lil Nas X","The Great Bear War: Grizzly Warrior","Polar Bear","Amory the Undead Necromancer", "Supreme Claus","Officer Blox", "Blocky Mech","Mr. Robot", "Police Officer Nash","Skelly", "Noob Attack","Snow Gentleman", "White Collar Dog", "Superhero", "Mr. Toilet", "Firefighter", "Magma Fiend", "Werner Weenie", "Penguin", "Borock the Conqueror", "Skeleton", "Gang O' Fries", "Gingerbread Man", "Robber", "Kid R6", "Kid R15"},
	Callback = function(Value)
    if Value == 'Korblox Mage' then
        game:GetService("ReplicatedStorage").Remotes.MorphEvent:FireServer("Bundle","Korblox Mage","R6")
    elseif Value == 'Fancy Mr. Fox' then
        game:GetService("ReplicatedStorage").HDAdminClient.Signals.RequestCommand:InvokeServer(prefix.."bundle me 404 ".."")
    elseif Value == 'Nick Bass' then
        game:GetService("ReplicatedStorage").HDAdminClient.Signals.RequestCommand:InvokeServer(prefix.."bundle me 360 ".."")
    elseif Value == 'Circuit Breaker' then
        game:GetService("ReplicatedStorage").HDAdminClient.Signals.RequestCommand:InvokeServer(prefix.."bundle me 162 ".."")
    elseif Value == 'Sun Slayer' then
        game:GetService("ReplicatedStorage").HDAdminClient.Signals.RequestCommand:InvokeServer(prefix.."bundle me 10 ".."")
    elseif Value == 'Vacation Rex' then
        game:GetService("ReplicatedStorage").HDAdminClient.Signals.RequestCommand:InvokeServer(prefix.."bundle me 327 ".."")
    elseif Value == 'Blue Collar Cat' then
        game:GetService("ReplicatedStorage").HDAdminClient.Signals.RequestCommand:InvokeServer(prefix.."bundle me 182 ".."")
    elseif Value == 'Cyclock' then
        game:GetService("ReplicatedStorage").HDAdminClient.Signals.RequestCommand:InvokeServer(prefix.."bundle me 611 ".."")
    elseif Value == 'Crazy Clown' then
        game:GetService("ReplicatedStorage").HDAdminClient.Signals.RequestCommand:InvokeServer(prefix.."bundle me 585 ".."")
