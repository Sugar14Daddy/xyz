        game:GetService("ReplicatedStorage").HDAdminClient.Signals.RequestCommand:InvokeServer(prefix.."bundle me 179 ".."")
    elseif Value == 'Temple Idol' then
        game:GetService("ReplicatedStorage").HDAdminClient.Signals.RequestCommand:InvokeServer(prefix.."r15 me ".."")
        game:GetService("ReplicatedStorage").HDAdminClient.Signals.RequestCommand:InvokeServer(prefix.."bundle me 938 ".."")
    elseif Value == 'Gothic Gargoyle' then
        game:GetService("ReplicatedStorage").HDAdminClient.Signals.RequestCommand:InvokeServer(prefix.."bundle me 446 ".."")
    elseif Value == 'Cyborg Shogun' then
        game:GetService("ReplicatedStorage").HDAdminClient.Signals.RequestCommand:InvokeServer(prefix.."bundle me 790 ".."")
    elseif Value == 'Lucky Gatito' then
        game:GetService("ReplicatedStorage").HDAdminClient.Signals.RequestCommand:InvokeServer(prefix.."bundle me 469 ".."")
    elseif Value == 'Rusty & Copper' then
        game:GetService("ReplicatedStorage").HDAdminClient.Signals.RequestCommand:InvokeServer(prefix.."bundle me 657 ".."")
