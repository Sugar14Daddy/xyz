--Functions

prefix = game.Players.LocalPlayer.PlayerGui.HDAdminGUIs.MainFrame.Pages.Settings.Custom["AE1 Prefix"].SettingValue.TextBox.Text 

local Players = game:GetService("Players")

function AutoFuckServer()
    while _G.AutoFuckServer == true do 
        wait(0000.1)
            game:GetService("ReplicatedStorage").Remotes.MorphEvent:FireServer("PetMorph","Cat")
            game:GetService("ReplicatedStorage").Remotes.MorphEvent:FireServer("PetMorph","Dog")
            game:GetService("ReplicatedStorage").Remotes.MorphEvent:FireServer("PetMorph","Pig")
            game:GetService("ReplicatedStorage").Remotes.MorphEvent:FireServer("PetMorph","Bunny")
            game:GetService("ReplicatedStorage").Remotes.MorphEvent:FireServer("PetMorph","Duck")
            game:GetService("ReplicatedStorage").Remotes.MorphEvent:FireServer("PetMorph","Flamingo")
            game:GetService("ReplicatedStorage").Remotes.MorphEvent:FireServer("PetMorph","Lion")
            game:GetService("ReplicatedStorage").Remotes.MorphEvent:FireServer("PetMorph","Wolf")
            game:GetService("ReplicatedStorage").Remotes.MorphEvent:FireServer("PetMorph","Griffin")
            game:GetService("ReplicatedStorage").Remotes.MorphEvent:FireServer("PetMorph","Unicorn")
            game:GetService("ReplicatedStorage").Remotes.MorphEvent:FireServer("PetMorph","Dragon")
        end
    end


local function chatted(msg)
    if msg == "/headless" then
        prefix = game.Players.LocalPlayer.PlayerGui.HDAdminGUIs.MainFrame.Pages.Settings.Custom["AE1 Prefix"].SettingValue.TextBox.Text 
        game:GetService("ReplicatedStorage").HDAdminClient.Signals.RequestCommand:InvokeServer(prefix.."bundle me 201 ".."")
        wait(0.1)
        game:GetService("ReplicatedStorage").Remotes.BasicFunction:InvokeServer("ResetOutfit")
        game:GetService("ReplicatedStorage").Remotes.MorphEvent:FireServer("ApplyColor",{["Left Leg"] = {Color3.new(1, 0.7522, 0.57961)},["Right Arm"] = {Color3.new(1, 0.7522, 0.57961)},["Head"] = {Color3.new(1, 0.7522, 0.57961)},["Torso"] = {Color3.new(1, 0.7522, 0.57961)},["Right Leg"] = {Color3.new(1, 0.7522, 0.57961)},["Left Arm"] = {Color3.new(1, 0.7522, 0.57961)}})
        wait(5)
    end
end

Players.LocalPlayer.Chatted:Connect(chatted)


function AutoMorphs()
    while _G.AutoMorphs == true do
loadstring(game:HttpGet("https://raw.githubusercontent.com/Sugar14Daddy/testBloodWave/main/Auto%20Morph", true))()
    end
end


function AntiGive()
    while _G.AntiGive == true do
        wait(0.2)
    for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do -- gets the children of the backpack
        if i >= 50 then -- checks if theres an index greater than or equal to 50
            wait(1)
game:GetService("ReplicatedStorage").Remotes.MorphEvent:FireServer("Reset") -- destroys your head to kill you
        else
        print(i,v) -- prints the index and value
        end
        end
    end
end


function Jokes()
    prefix = game.Players.LocalPlayer.PlayerGui.HDAdminGUIs.MainFrame.Pages.Settings.Custom["AE1 Prefix"].SettingValue.TextBox.Text 
    -- bundles game:GetService("ReplicatedStorage").HDAdminClient.Signals.RequestCommand:InvokeServer(prefix.."bundle me 192".."")
    
    prefix = game.Players.LocalPlayer.PlayerGui.HDAdminGUIs.MainFrame.Pages.Settings.Custom["AE1 Prefix"].SettingValue.TextBox.Text 
        wait(1)
        --1
    game:GetService("ReplicatedStorage").HDAdminClient.Signals.RequestCommand:InvokeServer(prefix.."hr ".."Jokes Time!")
        wait(1)
        --2
    game:GetService("ReplicatedStorage").HDAdminClient.Signals.RequestCommand:InvokeServer(prefix.."ho ".."What's the best thing about Switzerland?  =  i don't know, but the flag is a big plus. ")
        wait(1)
        --3
    game:GetService("ReplicatedStorage").HDAdminClient.Signals.RequestCommand:InvokeServer(prefix.."hg ".."Why do we tell actors to ''break a leg?''  =  because every play has a cast.")
        wait(1)
        --4
    game:GetService("ReplicatedStorage").HDAdminClient.Signals.RequestCommand:InvokeServer(prefix.."hdg ".."Knock! Knock! 'Who's there? 'Control freak. 'con... '0kay now you say, 'control freak who!")
        wait(1)
        --5
    game:GetService("ReplicatedStorage").HDAdminClient.Signals.RequestCommand:InvokeServer(prefix.."hpk ".."Y0 momma is so Du.mb she brought a spoon to the super bowl.")
        wait(1)
        --6
    game:GetService("ReplicatedStorage").HDAdminClient.Signals.RequestCommand:InvokeServer(prefix.."hb ".."Why do fathers take an extra pair of socks when they go golfing?'  'In case they get a hole in one!")
        wait(1)
        --7
    game:GetService("ReplicatedStorage").HDAdminClient.Signals.RequestCommand:InvokeServer(prefix.."hdb ".."A woman in labor suddenly shouted, 'shouldn't 'wouldn't 'couldn't 'didn't 'can't'  'Don't worry,' said the doc. Those are just contractions.'")
        wait(1)
