
--Family Paradise
local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Blood Ware   Family Paradise", HidePremium = true, IntroEnabled = false, IntroText = "BloodWare Hub", SaveConfig = true, ConfigFolder = "Blood Ware"})

